{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "brand": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "ean": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "website": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "active",
        "discontinued",
        "obsolete"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    }
  }
}
